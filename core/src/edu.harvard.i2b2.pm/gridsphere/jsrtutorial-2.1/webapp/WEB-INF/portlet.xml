<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">
    <portlet>
        <description xml:lang="en">Provides a persistent, searchable global param list</description>
        <description xml:lang="de"></description>
        <portlet-name>GlobalDataPortlet</portlet-name>
        <display-name xml:lang="en">Global Data Portlet</display-name>
        <display-name xml:lang="de">GlobalDataPortlet</display-name>
        <portlet-class>org.gridlab.gridsphere.jsrtutorial.portlets.globaldata.GlobalDataPortlet</portlet-class>
        <expiration-cache>60</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>GlobalData</title>
            <short-title>GlobalData</short-title>
            <keywords>GlobalData, persistence</keywords>
        </portlet-info>
    </portlet>
