<server>

  <!-- ==================================================================== -->
  <!-- JMS Stuff                                                            -->
  <!-- ==================================================================== -->

  <mbean code="org.jboss.mq.server.jmx.Queue"
	 name="jboss.mq.destination:service=Queue,name=jms/edu.harvard.i2b2.crc.loader.loadrunner">
    <attribute name="JNDIName">jms/edu.harvard.i2b2.crc.loader.loadrunner</attribute>
    <attribute name="RedeliveryLimit">0</attribute>
    <depends optional-attribute-name="DestinationManager">jboss.mq:service=DestinationManager</depends>
  </mbean>
</server>