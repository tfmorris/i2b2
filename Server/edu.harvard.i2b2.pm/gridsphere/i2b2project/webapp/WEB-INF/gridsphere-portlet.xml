<portlet-app-collection>

<portlet-app-def>
	<portlet-app id="edu.harvard.i2b2.pm.portlets.ProjectVariablePortlet">
	    <portlet-name>Group Variable Portlet Application</portlet-name>
	    <servlet-name>GroupVariable</servlet-name>
	    <portlet-config>
		<param-name>PortletConfig Variable</param-name>
		<param-value>some config parameter value</param-value>
	    </portlet-config>
	    <allows>
		<maximized/>
		<minimized/>
		<resizing/>
	    </allows>
	    <supports>
		<view/>
		<edit/>
		<help/>
		<configure/>
	    </supports>
	</portlet-app>

	<concrete-portlet-app id="edu.harvard.i2b2.pm.portlets.ProjectVariablePortlet.1">
	    <context-param>
		<param-name>A PortletApplicationSettings variable</param-name>
		<param-value>the value</param-value>
	    </context-param>
	    <concrete-portlet>
		<portlet-name>Group Variable</portlet-name>
		<default-locale>en</default-locale>
		<language locale="en_US">
		    <title>Project Parameter</title>
		    <title-short>Group Variable</title-short>
		    <description>Group Variable -  Portlet</description>
		    <keywords>portlet Group Variable</keywords>
		</language>
		<config-param>
		    <param-name>A PortletSettings parameter</param-name>
		    <param-value>the value</param-value>
		</config-param>
	    </concrete-portlet>
	</concrete-portlet-app>
</portlet-app-def>

